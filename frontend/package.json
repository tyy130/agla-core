    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@module-federation/nextjs-mf": "^8.8.54",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.2.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "webpack": "^5.105.0",
    "webpack-cli": "^6.0.1"
  }
